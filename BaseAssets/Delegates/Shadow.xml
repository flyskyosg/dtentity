<?xml  version="1.0" encoding="ISO-8859-1" standalone="no"?>
<delegates>
   <enuminput propertyname="ShadowTechnique" values="LISPSM|LISPSM_ViewBounds|LISPSM_CullBounds|PSSM" />

 <switchinput name="ShadowType">
     <switchgroup name="PSSM">
       <vec4property name="MapCount" />
     </switchgroup>

     <switchgroup name="LISPSM">
       <vec4property name="LISPPROP" />
     </switchgroup>
 </switchinput>


<arrayinput propertyname="Children" >
	   <prototype>
			<stringproperty name="child" />
	   </prototype>
	   <inputs>
	   <componentinput propertyname="child" />
	   </inputs>
   </arrayinput>

</delegates>   
