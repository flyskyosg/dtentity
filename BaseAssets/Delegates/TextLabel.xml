<?xml  version="1.0" encoding="ISO-8859-1" standalone="no"?>
<delegates>
   <arrayinput propertyname="Texts">
      <prototype>

         <groupproperty name="TextProperty">
            <stringproperty name="Text"></stringproperty>
            <vec4property name="Color" x="1" y="1" z="1" w="1" />
            <vec4property name="BackdropColor" x="1" y="0" z="0" w="1" />
            <boolproperty name="Visible" value="true" />
            <boolproperty name="Highlighted" value="false" />
            <vec3property name="Offset" x="0" y="0" z="0" />
            <floatproperty name="CharacterHeight" value="30" />
            <stringproperty name="Font">TrueTypeFonts/FreeSans.ttf</stringproperty>
			<vec2property name="PixelOffset" x="0" y="0" />
			<stringproperty name="Alignment">CENTER_CENTER</stringproperty>
         </groupproperty>

      </prototype>
 
      <inputs>
         <groupinput propertyname="*">
            <colorinput propertyname="Color" />
			<colorinput propertyname="BackdropColor" />
			<enuminput propertyname="Alignment" values="LEFT_TOP|LEFT_CENTER|LEFT_BOTTOM|CENTER_TOP|CENTER_CENTER|CENTER_BOTTOM|RIGHT_TOP|RIGHT_CENTER|RIGHT_BOTTOM|LEFT_BASE_LINE|CENTER_BASE_LINE|RIGHT_BASE_LINE|LEFT_BOTTOM_BASE_LINE|CENTER_BOTTOM_BASE_LINE|RIGHT_BOTTOM_BASE_LINE" />
         </groupinput>
      </inputs>
   </arrayinput> 
</delegates>   