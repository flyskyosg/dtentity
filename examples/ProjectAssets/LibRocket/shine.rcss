datagrid datagridheader
{
	width: auto;
	height: 25px;
	padding: 5px 10px 0px 10px;

	background-decorator: tiled-horizontal;
	background-left-image: shine.tga 127px 192px 143px 223px;
	background-center-image: shine.tga stretch 143px 192px 145px 223px;
	background-right-image: shine.tga 145px 192px 160px 223px;
}

