body
{
	font-family: Delicious;
	font-weight: normal;
	font-style: normal;
	font-size: 15;
	color: #303030;
}

body.window
{	
	z-index: top;
	min-width: 250px;
	max-width: 800px;
	
	min-height: 135px;
	max-height: 700px;
	padding-bottom:64px;
	
	background-decorator: tiled-box;
	background-top-left-image:     shine.tga           0px  0px 17px 50px;
	background-top-image:          shine.tga stretch  18px  0px 52px 50px;
	background-top-right-image:    shine.tga          53px  0px 70px 50px;
	background-left-image:         shine.tga stretch   0px 30px 17px 54px;
	background-center-image:       shine.tga stretch  18px 31px 52px 53px;
	background-right-image:        shine.tga stretch  53px 30px 70px 54px;
	background-bottom-left-image:  shine.tga           0px 54px 17px 71px;
	background-bottom-image:       shine.tga stretch  17px 54px 54px 71px;
	background-bottom-right-image: shine.tga          53px 54px 70px 71px;	
}

div#title_bar
{
	
	text-align: left;
	font-family: Delicious;
	font-weight: normal;
	font-style: normal;
	font-size: 10;
	color: #303030;

	height: 30px;
	
	/*background-decorator: tiled-horizontal;
	background-left-image: shine.tga 0px 0px 17px 30px;
	background-center-image: shine.tga stretch 18px 0px 52px 30px;
	background-right-image: shine.tga 53px 0px 70px 30px;*/
}

div#mover
{
	

}
div#title
{
	margin-top:10px;
	padding-left: 17px;
	padding-right: 17px;
	padding-top:2px;
	font-size: 12px;	

}


div#window
{
	padding-top: 0px;
	padding-bottom: 10px;
	padding-left: 17px;
	padding-right: 17px;
}

div#scrollbar
{
	overflow: hidden auto;
	width: auto;
	height: 100%;
	
}

div#spacer
{
	background-color:#FFFFFF88;
	padding-right: 12px;
	padding-top: 6px;
	padding-bottom: 10px;
}
div#content
{  
	padding-top: 10px;
	padding-bottom: 10px;
	padding-left: 10px;
	padding-right: 10px;
	
	text-align: center;
	
}

p
{
	text-align: left;
	margin-bottom: 1em;
}

h1
{
	margin-left: 0.4em;
	margin-bottom: 0.4em;

	text-align: left;
	font-size: 16;
	font-weight: bold;

	shadow-font-effect: shadow;
	shadow-offset: 1px 1px;
	shadow-color: black;
}



input,
select,
dataselect
{
	
}

input.submit
{
}



button,
input.submit
{

	display: inline-block;

	width: 90px;
	height: 22px;


	padding-left:0px;
	padding-top: 4px;
	padding-bottom: 0px;
	padding-right: 0px;


	font-size: 12;
	text-align: center;
	tab-index: auto;

	background-decorator: image;
	background-image: shine.tga 66px 78px 156px 100px;
}

button:focus,
input.submit:focus
{
    font-weight: bold;
    
    shadow-font-effect: shadow;
    shadow-offset: 1px 1px;
    shadow-color: black;
}

button:hover,
input.submit:hover
{
	background-image: shine.tga 66px 103px 156px 125px;
}

button:active,
input.submit:active
{
	background-image: shine.tga 66px 128px 156px 150px;
}



input.text
{
	height: 22px;
	padding: 3px 10px 0px;

	background-decorator: tiled-horizontal;
	background-left-image: shine.tga 41px 113px 43px 131px;
	background-right-image: shine.tga 56px 113px 58px 131px;
	background-center-image: shine.tga stretch 43px 113px 56px 131px;
}

input.text,
select,
dataselect
{
	color: black;
	font-size: 12;
}

datagrid input.text
{
	width: 100%;
	height: auto;
	margin: 0px;
	padding: 0px 2px 0px;

	border-width: 1px;
	border-color: black;
	background-color: white;

	font-size: 15;
	
	background-decorator: none;
}



select,
dataselect
{
	width: 90px;
	height: 22px;
}

select selectvalue,
dataselect selectvalue
{
	width: auto;
	
	height: 22px;
	padding-left: 9px;
	padding-right: 2px;

	background-decorator: image;
	background-image: shine.tga 66px 78px 156px 100px;
}

select selectvalue:hover,
dataselect selectvalue:hover
{
	icon-image: shine.tga 5px 108px 15px 115px;
}

select selectarrow,
dataselect selectarrow
{
	width: 15px;
	height: 18px;
	
	icon-decorator: image;
	icon-image: shine.tga 5px 107px 20px 125px;
}

select selectarrow:hover,
dataselect selectarrow:hover
{
	icon-image: shine.tga 24px 107px 39px 125px;
}

select selectarrow:active,
select selectarrow:checked,
dataselect selectarrow:active,
dataselect selectarrow:checked
{
	icon-image: shine.tga 24px 107px 39px 125px;
}

select selectbox,
dataselect selectbox
{
	margin-left: 1px;
	margin-top: 0px;
	width: 162px;
	padding: 1px 4px 4px 4px;
}

select selectbox,
dataselect selectbox,
datagrid datagridbody
{
	background-decorator: tiled-box;
	background-top-left-image: shine.tga 41px 113px 43px 115px;
	background-top-right-image: shine.tga 55px 113px 58px 115px;
	background-top-image: shine.tga stretch 44px 113px 55px 115px;
	background-bottom-image: shine.tga stretch 43px 128px 56px 132px;
	background-bottom-left-image: shine.tga 41px 128px 43px 132px;
	background-left-image: shine.tga 41px 115px 43px 128px;
	background-right-image: shine.tga 56px 116px 58px 128px;
	background-bottom-right-image: shine.tga 56px 128px 58px 132px;
	background-center-image: shine.tga stretch 42px 114px 57px 130px;
}

select selectbox option,
dataselect selectbox option
{
	width: auto;
	padding-left: 3px;
}


